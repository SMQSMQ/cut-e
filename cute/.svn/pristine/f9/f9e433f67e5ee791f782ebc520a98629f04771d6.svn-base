/**壁纸板块**/
        .left-fixed {
            display: -webkit-box;
            position: fixed;
            left: 0px;
            width: 100%;
            top: 40px;
        }
        .left-fixed>div {
            display: -webkit-box;
            -webkit-box-flex: 1;
        }
        .left-fixed>div.left {
            width: 60px;
            -webkit-box-flex: 0;
            border-top: 1px solid #2468c9;
            border-bottom: 1px solid #fff;
            background: #2773dc;
            line-height: 34px;
            display: none;
        }
        .left-fixed>div.left a {
            color: #fff;
            display: block;
            width: 100%;
            text-align: center;
            height: 100%;
        }
        .left-fixed>div.left a.ui-state-active {
            background: #0c4da8;
        }
        .ui-scroller {
            float: left;
        }

.share-shade{
  position: fixed;
  margin: auto;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0,0,0,.6);
  z-index: 15;
  top: 100%;
  transition: top ease .3s;
}
.share-shade.up{
  top: 0;
}
.share-inner{
  width: 90%;
  position: fixed;
  margin: auto;
    border-radius: 6px;
  -ms-border-radius: 6px;
  -webkit-border-radius: 6px;
  -o-border-radius: 6px;
  -moz-border-radius: 6px;
  top: 100%;
  left: 0;
  right: 0;
  text-align: center;
  background: #fff;
  z-index: 20;
  transition: top ease .3s;
}
.share-inner.up{
  top: 50%;
  transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  -ms-transform: translateY(-50%);

}
.share-inner h2{
      border-radius: 6px 6px 0 0 ;

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>CuteYep</title>
</head>
<style>
   html,body,img{
     width: 100%;
     overflow-x: hidden;
   }
   #out{
     text-align:center;
     display: none;
   }
   #out a{
     display: inline-block;
     padding: 15px 20px;
     border-radius: 15px;
     -webkit-border-radius: 15px;
     -moz-border-radius: 15px;
     -o-border-radius: 15px;
     -ms-border-radius: 15px;
     background: #ee436b;
     color: #fff;
   }
</style>
<body>
  <img id="img" src="__APP__/public/images/llq.png" alt="">
  <div id="out">
    <h2>打开CuteYep有耶APP</h2>
    <a id="open">点击打开CuteYep有耶APP</a>
  </div>

</body>
</html>

<script>
  var tipId = '{$tipid}';
  if(tipId == 3){
    // window.location.href = 'http://www.cuteyep.com/h5/index.php?s=Index/out';
    document.getElementById('out').style.display = 'block';
    document.getElementById('img').style.display = 'none';

    document.getElementById('open').onclick = function(e){
      var ifr = document.createElement('iframe');
      ifr.src = 'dongyuan://cuteyep.com/cy?openId=Index/expression_con/id/{$see.id}';
      ifr.style.display = 'none';
      document.body.appendChild(ifr);
      window.setTimeout(function(){
          document.body.removeChild(ifr);
          window.location.href="http://a.app.qq.com/o/simple.jsp?pkgname=com.ct.cute";
      },1000)
    };
  }
</script>
